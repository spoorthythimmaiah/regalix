<div class="col-md-6 col-sm-6 col-xs-12 pwt-wrap">
    <div class="pwt-box " slug="{{section.slug}}">
        <div class="left icon">
            <div class="outer-circle">
                <div class="inner-circle progress">
                    <div class="icon-down-arrow">
                        <img src="{{icon}}">
                    </div>
                </div>
            </div>
        </div>
        <div class="right">
            <div class="title">{{section.name}}</div>
            <div class="pwt-details">{{section.description}}</div>
        </div>
    </div>
</div>
